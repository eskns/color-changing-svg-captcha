{
  "name": "@eskns/color-changing-svg-captcha",
  "version": "0.1.1",
  "description": "generate svg captcha in node.js and dynamically color it in the browser for each keystroke.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "xo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eskns/color-changing-svg-captcha.git"
  },
  "keywords": [
    "captcha",
    "svg",
    "node captcha",
    "captcha generator",
    "captcha alternative",
    "dynamic colors in the browser",
    "better end-user readability",
    "lower captcha errors"
  ],
  "author": "herbi.n.ginger <herbi.n.ginger@gmail.com>",
  "engines": {
    "node": ">=4.x"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/eskns/color-changing-svg-captcha/issues"
  },
  "homepage": "https://github.com/eskns/color-changing-svg-captcha/blob/master/README.md",
  "dependencies": {
    "opentype.js": "^1.3.4"
  },
  "devDependencies": {
    "jest": "^29.3.1",
    "jest-environment-node-debug": "^2.0.0",
    "xo": "^0.53.1"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "jest"
    ],
    "rules": {
      "linebreak-style": [
        0
      ]
    }
  },
  "typings": "index.d.ts",
  "files": [
    "fonts/*",
    "lib/*",
    "index.d.ts",
    "*.md"
  ]
}
